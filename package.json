{
  "name": "grafana-iot-sitewise-datasource",
  "version": "1.5.1",
  "description": "View IoT Sitewise data in grafana",
  "scripts": {
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "build": "rm -rf dist && grafana-toolkit plugin:build && mage build:backend",
    "test": "grafana-toolkit plugin:test",
    "lint": "grafana-toolkit plugin:lint",
    "dev": "rm -rf node_modules/@grafana/data/node_modules; grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch"
  },
  "repository": "github:grafana/iot-sitewise-datasource",
  "author": "Grafana Labs <team@grafana.com> (https://grafana.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/aws-sdk": "^0.0.37",
    "@grafana/data": "8.5.0",
    "@grafana/runtime": "8.5.0",
    "@grafana/toolkit": "8.5.0",
    "@grafana/ui": "8.5.0",
    "@types/lodash": "latest",
    "cspell": "6.13.3",
    "lodash": "latest",
    "tslib": "2.2.0"
  },
  "resolutions": {
    "rxjs": "6.6.3"
  }
}
