{
  "name": "grafana-iot-sitewise-datasource",
  "version": "2.0.0",
  "description": "View IoT Sitewise data in grafana",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "playwright test",
    "e2e:debug": "npx playwright test --ui",
    "generate-release-notes": "PREV_TAG=$(git tag | tail -n 1) && gh api --method POST /repos/grafana/iot-sitewise-datasource/releases/generate-notes -f tag_name=v${npm_package_version} -f target_commitish=main -f previous_tag_name=${PREV_TAG} | jq -r .body",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix && prettier --write --list-different .",
    "server": "docker compose up --build",
    "server:dev": "DEVELOPMENT=true yarn server",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "test:coverage": "jest --coverage",
    "test:coverage:changes": "jest --coverage --changedSince=origin/main",
    "typecheck": "tsc --noEmit"
  },
  "repository": "github:grafana/iot-sitewise-datasource",
  "author": "Grafana Labs <team@grafana.com> (https://grafana.com)",
  "license": "Apache-2.0",
  "dependencies": {
    "@grafana/data": "^11.5.0",
    "@grafana/plugin-ui": "^0.9.4",
    "@grafana/runtime": "^11.5.0",
    "@grafana/schema": "^11.4.0",
    "@grafana/ui": "^11.4.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "^7.1.5",
    "tslib": "2.8.1",
    "@emotion/css": "11.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@grafana/aws-sdk": "0.6.0",
    "@grafana/eslint-config": "^8.0.0",
    "@grafana/plugin-e2e": "^1.17.1",
    "@grafana/tsconfig": "^2.0.0",
    "@playwright/test": "^1.50.1",
    "@swc/core": "^1.10.12",
    "@swc/helpers": "^0.5.15",
    "@swc/jest": "^0.2.37",
    "@testing-library/dom": "^10.3.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.15",
    "@types/node": "^22.13.0",
    "@types/react-router-dom": "^5.2.0",
    "copy-webpack-plugin": "^12.0.2",
    "cspell": "^8.17.3",
    "css-loader": "^7.1.2",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-webpack-plugin": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^11.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lefthook": "^1.10.10",
    "lodash": "^4.17.21",
    "prettier": "^3.4.2",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.83.4",
    "sass-loader": "16.0.4",
    "style-loader": "4.0.0",
    "swc-loader": "^0.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-livereload-plugin": "^3.0.2",
    "@stylistic/eslint-plugin-ts": "^3.0.1",
    "@types/testing-library__jest-dom": "6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "imports-loader": "^5.0.0",
    "semver": "^7.7.0",
    "terser-webpack-plugin": "^5.3.11",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2"
  },
  "resolutions": {
    "cosmiconfig": "8.3.6",
    "jackspeak": "2.1.1"
  },
  "packageManager": "yarn@1.22.19"
}
